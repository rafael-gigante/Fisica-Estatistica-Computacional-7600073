set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 600, 400 
set term png
set output 'scatter.1.jpg'
set grid
set parametric
set autoscale
set view 65, 45
set title "Modelo de crescimento DLA em três dimensões" 
set style data points
unset key


splot "seed.out" with points pt 7 ps 0.75 lc rgb "#ad2060"